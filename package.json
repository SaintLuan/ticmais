{
  "name": "front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "yarn && yarn clear && next build",
    "start": "node server.js",
    "lint": "next lint",
    "pm2:reset": "pm2 -s delete ticmais-store--prod && pm2 save || true",
    "pm2:start": "pm2 start 'yarn start' --name 'ticmais-store--prod' && pm2 save",
    "deploy:prod": "yarn build && yarn pm2:reset && yarn pm2:start"
  },
  "dependencies": {
    "@next/font": "13.1.0",
    "@types/node": "18.11.17",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "@types/styled-components": "^5.1.26",
    "babel-plugin-styled-components": "^2.0.7",
    "custom-env": "^2.0.1",
    "eslint": "8.30.0",
    "eslint-config-next": "13.1.0",
    "next": "13.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-iconly": "^2.2.5",
    "styled-components": "^5.3.6",
    "typescript": "4.9.4"
  }
}
